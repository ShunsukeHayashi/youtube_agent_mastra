{
  "name": "youtube-mastra-agent",
  "version": "1.0.0",
  "description": "YouTube AI Agent built with Mastra.ai framework",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "mastra dev",
    "build": "tsc && mastra build",
    "start": "node dist/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch --config jest.config.cjs",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --config jest.config.cjs",
    "demo:streaming": "npx tsx src/streaming/streamingDemo.ts",
    "test:streaming": "npx tsx test-streaming.ts"
  },
  "keywords": [
    "youtube",
    "mastra",
    "ai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@mastra/core": "^0.10.0",
    "@mastra/loggers": "^0.10.0",
    "@mastra/memory": "^0.10.0",
    "ai": "^3.4.33",
    "dotenv": "^16.5.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^149.0.0",
    "zod": "^3.25.20"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "jest": "^29.7.0",
    "mastra": "^0.10.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
